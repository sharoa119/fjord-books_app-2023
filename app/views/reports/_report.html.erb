<div id="<%= dom_id report %>">
  <p>
    <strong>Title:</strong>
    <%= report.title %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= report.content %>
  </p>

  <p>
    <strong>User:</strong>
    <%= report.user.name %>
  </p>
  <p>
    <strong><%= Report.human_attribute_name(:created_at) %>:</strong>
    <%= l report.created_at, format: :short %>
  </p>

</div>
